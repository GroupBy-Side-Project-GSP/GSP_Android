<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.MyPageActivity">


    <data>


    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.main.MyPageActivity">

        <TextView
            android:id="@+id/TvTittleMyPage"
            android:layout_width="83dp"
            android:layout_height="27dp"
            android:fontFamily="@font/notosan_font"
            android:textColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/BtnMySetting"
            android:text="마이페이지"
            android:textSize="18sp"
            android:gravity="center"
            android:layout_marginTop="8dp"
            android:layout_marginStart="20dp"
            android:includeFontPadding="false"
            >
        </TextView>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/BtnMySetting"
            android:layout_width="16dp"
            android:layout_height="16dp"
            app:layout_constraintStart_toEndOf="@id/TvTittleMyPage"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/ic_mypage_settings"
            android:layout_marginStart="221dp"
            />

        <ImageView
            android:id="@+id/IvProFilePhoto"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/TvTittleMyPage"
            app:layout_constraintEnd_toStartOf="@id/TvName"
            android:layout_marginStart="20dp"
            android:layout_marginTop="21dp"
            android:background="#009688"
            />

        <TextView
            android:id="@+id/TvName"
            android:layout_width="50dp"
            android:layout_height="24dp"
            android:includeFontPadding="false"
            android:text="testname"
            android:fontFamily="@font/notosan_font"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:gravity="center"
            app:layout_constraintEnd_toStartOf="@id/TvInterestExplan"
            app:layout_constraintStart_toEndOf="@id/IvProFilePhoto"
            app:layout_constraintTop_toBottomOf="@id/TvTittleMyPage"
            android:layout_marginStart="16dp"
            android:layout_marginTop="29dp"
            />

        <TextView
            android:id="@+id/TvInterestExplan"
            android:layout_width="38dp"
            android:layout_height="20dp"
            android:background="@drawable/my_page_interest"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@id/TvName"
            app:layout_constraintTop_toBottomOf="@id/TvTittleMyPage"
            android:layout_marginStart="8dp"
            android:layout_marginTop="31dp"
            android:layout_marginEnd="188dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:includeFontPadding="false"
            />

        <View
            android:id="@+id/Vline"
            android:layout_width="match_parent"
            android:layout_height="4dp"
            android:background="@color/light_gray"
            app:layout_constraintTop_toBottomOf="@id/IvProFilePhoto"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="16dp"
            />

        <LinearLayout
            android:id="@+id/LinMyProEdite"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintTop_toTopOf="@id/Vline"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/ConMyProEdite"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/BtnMyProfileEdide"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/ic_mypage_edit"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/TvMyProfileEdide"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    />
                <TextView
                    android:id="@+id/TvMyProfileEdide"
                    android:layout_width="68dp"
                    android:layout_height="20dp"
                    app:layout_constraintStart_toEndOf="@id/BtnMyProfileEdide"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:gravity="start"
                    android:includeFontPadding="false"
                    android:layout_marginBottom="11dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginEnd="244dp"
                    android:text="프로필 수정"
                    android:fontFamily="@font/notosan_font"
                    android:textSize="14sp"
                    android:textColor="@color/black"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinMyOffice"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintTop_toBottomOf="@id/LinMyProEdite"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/ConMyOffice"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/BtnMyOffice"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/ic_mypage_star"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/TvMyOffice"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    />
                <TextView
                    android:id="@+id/TvMyOffice"
                    android:layout_width="68dp"
                    android:layout_height="20dp"
                    app:layout_constraintStart_toEndOf="@id/BtnMyOffice"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:gravity="start"
                    android:includeFontPadding="false"
                    android:layout_marginBottom="11dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginEnd="244dp"
                    android:text="즐겨찾기한 회사"
                    android:fontFamily="@font/notosan_font"
                    android:textSize="14sp"
                    android:textColor="@color/black"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinMyInterest"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintTop_toBottomOf="@id/LinMyOffice"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/ConMyInterest"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/BtnMyInterest"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/ic_mypasge_tune"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/TvMyInterest"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    />
                <TextView
                    android:id="@+id/TvMyInterest"
                    android:layout_width="68dp"
                    android:layout_height="20dp"
                    app:layout_constraintStart_toEndOf="@id/BtnMyInterest"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:gravity="start"
                    android:includeFontPadding="false"
                    android:layout_marginBottom="11dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginEnd="244dp"
                    android:text="관심분야 설정"
                    android:fontFamily="@font/notosan_font"
                    android:textSize="14sp"
                    android:textColor="@color/black"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinMyNotion"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintTop_toBottomOf="@id/LinMyInterest"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/ConMyNotion"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/BtnMyNotion"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/ic_mypage_mic"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/TvMyNotion"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    />
                <TextView
                    android:id="@+id/TvMyNotion"
                    android:layout_width="68dp"
                    android:layout_height="20dp"
                    app:layout_constraintStart_toEndOf="@id/BtnMyNotion"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:gravity="start"
                    android:includeFontPadding="false"
                    android:layout_marginBottom="11dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginEnd="244dp"
                    android:text="공지사항"
                    android:fontFamily="@font/notosan_font"
                    android:textSize="14sp"
                    android:textColor="@color/black"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinMyAppSetting"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            app:layout_constraintTop_toBottomOf="@id/LinMyNotion"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/ConMyAppSetting"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/BtnMyAppSetting"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/ic_mypage_settings2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/TvMyAppSetting"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    />
                <TextView
                    android:id="@+id/TvMyAppSetting"
                    android:layout_width="68dp"
                    android:layout_height="20dp"
                    app:layout_constraintStart_toEndOf="@id/BtnMyAppSetting"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:gravity="start"
                    android:includeFontPadding="false"
                    android:layout_marginBottom="11dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginEnd="244dp"
                    android:text="앱 설정"
                    android:fontFamily="@font/notosan_font"
                    android:textSize="14sp"
                    android:textColor="@color/black"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
















    </androidx.constraintlayout.widget.ConstraintLayout>



</layout>